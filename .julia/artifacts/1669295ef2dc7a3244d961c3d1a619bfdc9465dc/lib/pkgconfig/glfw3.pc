prefix=/workspace/destdir
exec_prefix=${prefix}
includedir=/workspace/destdir/include
libdir=/workspace/destdir/lib

Name: GLFW
Description: A multi-platform library for OpenGL, window and input
Version: 3.3.9
URL: https://www.glfw.org/
Requires.private:  x11
Libs: -L${libdir} -lglfw
